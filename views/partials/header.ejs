    <script src="/vendor/ckeditor/ckeditor.js"></script>
    <link rel="stylesheet" href="/css/style.css">
</head>


<body id="page-top">

    <!-- Page Wrapper -->
    <div id="wrapper">

        <!-- Sidebar -->
        <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion" id="accordionSidebar">

            <!-- Sidebar - Brand -->
            <a class="sidebar-brand d-flex align-items-center justify-content-center" href="/dashboard">
                <div class="sidebar-brand-icon">
                    <img src="/img/XEA_rev.png" class="sidebar-brand sidebar-img text-white" alt="">
                </div>
            </a>

            <!-- Divider -->
            <hr class="sidebar-divider my-0">

            <li class="nav-item" onmousewheel="intro.start();" data-intro='Click here to know more'
                data-position='bottom'>
                <a class="nav-link" href="/dashboard">
                    <i class="fas fa-fw fa-tachometer-alt"></i>
                    <span>Dashboard</span></a>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider">

            <!-- Nav Item - Pages Collapse Menu -->
            <li class="nav-item" id="placetest" data-intro='Click here to know more' data-position='bottom'>
                <a class="nav-link collapsed" href="/placementtest" data-toggle="collapse" data-target="#collapseTwo"
                    aria-expanded="true" aria-controls="collapseTwo">
                    <i class="fas fa-fw fa-cog"></i>
                    <span>Inbox Placement Test</span>
                </a>
                <div id="collapseTwo" class="collapse" aria-labelledby="headingTwo" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <a class="collapse-item" href="/manualplacementtest">Run Test</a>
                        <a class="collapse-item" href="/autotest">Automate Test</a>
                        <a class="collapse-item" href="/manualtesthistory">Test History</a>
                        <!-- show to only admin -->
                        <a class="collapse-item" href="/managesmtp">Manage SMTP</a>
                        <% if(user.type=="ADMIN" ) { %>
                            <a class="collapse-item" href="/manageimap">Manage IMAP</a>
                            <% } %>
                    </div>
                </div>
            </li>
            <li class="nav-item" id="blacklist" data-intro='Click here to know more' data-position='bottom'>
                <a class="nav-link collapsed" href="/monitoring" data-toggle="collapse" data-target="#billing1"
                    aria-expanded="true" aria-controls="collapseUtilities">
                    <i class="fas fa-fw fa-wrench"></i>
                    <span>Monitoring</span>
                </a>
                <div id="billing1" class="collapse" aria-labelledby="headingUtilities" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <a class="collapse-item" href="/ipblacklist">IP Blacklist</a>
                        <a class="collapse-item" href="/ipblacklist/report">IP Blacklist Report</a>
                        <a class="collapse-item" href="/ipblacklist/myipaddresses">My IP Addresses</a>

                    </div>
                </div>
            </li>
            <!-- Nav Item - Utilities Collapse Menu -->
            <li class="nav-item" id="dns" data-intro='Click here to know more' data-position='bottom'>
                <a class="nav-link collapsed" href="/dns-checker" data-toggle="collapse"
                    data-target="#collapseUtilities" aria-expanded="true" aria-controls="collapseUtilities">
                    <i class="fas fa-fw fa-wrench"></i>
                    <span>Deliverablity DNS Records</span>
                </a>
                <div id="collapseUtilities" class="collapse" aria-labelledby="headingUtilities"
                    data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <a class="collapse-item" href="/reverseip-checker">Reserve IP</a>
                        <a class="collapse-item" href="/ping-checker">Ping Record Validator</a>
                        <a class="collapse-item" href="/dns-checker">DNS Record Validator</a>
                        <a class="collapse-item" href="/dmarc-inspector">DMARC Record Validator</a>
                        <a class="collapse-item" href="/dkim-checker">DKIM Record Validator</a>
                        <a class="collapse-item" href="/spf-checker">SPF Record Validator</a>
                    </div>
                </div>
            </li>

            <!-- Divider -->
            <hr class="sidebar-divider">

            <!-- Heading -->
            <div class="sidebar-heading">
                Billing Section
            </div>

            <li class="nav-item" id="paydetail" data-intro='Click here to know more' data-position='bottom'>
                <a class="nav-link collapsed" href="/paydetails" data-toggle="collapse" data-target="#billing"
                    aria-expanded="true" aria-controls="collapseUtilities">
                    <i class="fas fa-fw fa-wrench"></i>
                    <span>Billing</span>
                </a>
                <div id="billing" class="collapse" aria-labelledby="headingUtilities" data-parent="#accordionSidebar">
                    <div class="bg-white py-2 collapse-inner rounded">
                        <a class="collapse-item" href="/paydetails">Pay As You Go</a>
                        <a class="collapse-item" href="/subscription">Subscription</a>
                        <a class="collapse-item" href="/invoices">Invoice</a>
                    </div>
                </div>
            </li>

            <% if(user.type=="ADMIN" ) { %>
                <!-- Divider -->
                <hr class="sidebar-divider">

                <!-- Heading -->
                <div class="sidebar-heading">
                    User Manager
                </div>

                <li class="nav-item" data-intro='Click here to know more' data-position='bottom'>
                    <a class="nav-link collapsed" href="#" data-toggle="collapse" data-target="#usermanager"
                        aria-expanded="true" aria-controls="collapseUtilities">
                        <i class="fas fa-fw fa-wrench"></i>
                        <span>Manage Users</span>
                    </a>
                    <div id="usermanager" class="collapse" aria-labelledby="headingUtilities"
                        data-parent="#accordionSidebar">
                        <div class="bg-white py-2 collapse-inner rounded">
                            <a class="collapse-item" href="/users">All Users</a>
                        </div>
                    </div>
                </li>
                <% } %>

                    <!-- Divider -->
                    <hr class="sidebar-divider d-none d-md-block">

                    <!-- Sidebar Toggler (Sidebar) -->
                    <div class="text-center d-none d-md-inline">
                        <button class="rounded-circle border-0" id="sidebarToggle"></button>
                    </div>

        </ul>
        <!-- End of Sidebar -->

        <!-- Content Wrapper -->
        <div id="content-wrapper" class="d-flex flex-column">

            <!-- Main Content -->
            <div id="content">

                <!-- Topbar -->
                <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">

                    <!-- Sidebar Toggle (Topbar) -->
                    <button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3">
                        <i class="fa fa-bars"></i>
                    </button>


                    <div id="credit" data-intro='Click here to know more' data-position='bottom'>
                        <h5>Credit Balance: <%= user.emailCount %>
                        </h5>
                    </div>
                    <!-- Topbar Navbar -->
                    <ul class="navbar-nav ml-auto">
                        <!-- Nav Item - Alerts -->
                        <li class="nav-item m-3">
                            <% if(user.type=='ADMIN' ) { %>
                                <a class="btn btn-outline-primary" href="/admin">
                                    Admin Dashboard
                                </a>
                                <% } %>
                        </li>
                        <div class="topbar-divider d-none d-sm-block"></div>

                        <!-- Nav Item - User Information -->
                        <li class="nav-item dropdown no-arrow" data-intro='Click here to know more'
                            data-position='bottom'>
                            <a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button"
                                data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <span class="mr-2 d-none d-lg-inline text-gray-600 small">
                                    <%= user.firstname %>
                                </span>
                                <img class="img-profile rounded-circle" src="/img/undraw_profile.svg">
                            </a>
                            <!-- Dropdown - User Information -->
                            <div class="dropdown-menu dropdown-menu-right shadow animated--grow-in"
                                aria-labelledby="userDropdown">
                                <a class="dropdown-item" href="/profile">
                                    <i class="fas fa-user fa-sm fa-fw mr-2 text-gray-400"></i>
                                    Profile
                                </a>
                                <a class="dropdown-item" href="/settings">
                                    <i class="fas fa-cogs fa-sm fa-fw mr-2 text-gray-400"></i>
                                    Settings
                                </a>
                                <div class="dropdown-divider"></div>
                                <a class="dropdown-item" href="/logout" data-toggle="modal" data-target="#logoutModal">
                                    <i class="fas fa-sign-out-alt fa-sm fa-fw mr-2 text-gray-400"></i>
                                    Logout
                                </a>
                            </div>
                        </li>

                    </ul>
                    <br> <button class="btn btn-primary" onclick="starttour()">START TOUR</button>
                </nav>

